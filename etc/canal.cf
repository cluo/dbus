{
    apisvr_addr: "127.0.0.1:9876"
    influx_addr: "http://localhost:8086"

    plugins: [
        {
            name: "binlog"
            class: "MysqlbinlogInput"
            dsn: "local://root:@localhost:3306"
            db_excluded: ["bar", ]
        }            

        {
            name: "binlog_out"
            class: "KafkaOutput"
            dsn: "local://me/foobar"
            async: true
            match: ["binlog", ]
        }
    ]

}
