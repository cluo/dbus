{
    influx_addr: "http://localhost:8086"

    plugins: [
        {
            name: "in.binlog"
            class: "MysqlbinlogInput"
            dsn: "local://root:@localhost:3306"
            db_excluded: ["bar", ]
        }            

        {
            name: "out.kafka"
            class: "KafkaOutput"
            reporter: true
            dsn: "local://me/foobar"
            match: ["in.binlog", ]
        }
    ]

}
