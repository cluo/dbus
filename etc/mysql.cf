{
    api_addr: "127.0.0.1:9876"
    influx_addr: "http://localhost:8086"
    influx_db: "dbus"
    influx_tick: "1m"

    projects: [
        {
            name: "mysql"
        }
    ]

    plugins: [
        {
            name: "binlog"
            class: "MysqlbinlogInput"
            // zone is used for positioner
            zone: "local"
            pos_commit_interval: "1s"
            event_buffer_len: 100
            master_host: "localhost"
            master_port: 3306
            user: "root"
            password: ""
            server_id: 137
            GTID: false
            table_excluded: ["foo", ]
            db_excluded: ["bar", ]
        }            

        {
            name: "binlog_out"
            class: "KafkaOutput"
            load_position: true
            zone: "local"
            cluster: "me"
            topic: "foobar"
            ack: 1
            async: false
            compress: false
            hhdirs: ["hh", ]
            match: ["binlog", ]
        }
    ]

}
